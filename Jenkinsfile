    stage('asda') {
      parallel {
        stage('asda') {
          steps {
            sh 'echo ""'
          }
        }

        stage('sdf ') {
          steps {
            echo 'xzds '
          }
        }

      }
    }

    stage('sadff') {
      steps {
        echo 'sdf'
      }
    }

  }
  environment {
